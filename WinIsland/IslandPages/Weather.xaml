<Page x:Class="WinIsland.IslandPages.Weather"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WinIsland.IslandPages"
      mc:Ignorable="d" 
      Loaded="Page_Loaded"
      d:DesignHeight="370" d:DesignWidth="872"
      Title="Weather">

    <Grid Background="Cyan" Margin="0,35,0,0">
        <StackPanel>
            <Label x:Name="elevation" Visibility="Hidden"/>

            <Label x:Name="time"/>
            <Label x:Name="weathercodes"/>


        </StackPanel>
    </Grid>
</Page>
